#include <iostream>
using namespace std;

int main() {
    int num1 = 5;
    int num2 = 10;
    int temp;

    cout << "Before swapping:" << endl;
    cout << "num1 = " << num1 << endl;
    cout << "num2 = " << num2 << endl;

    // Swapping
    temp = num1;
    num1 = num2;
    num2 = temp;

    cout << "After swapping:" << endl;
    cout << "num1 = " << num1 << endl;
    cout << "num2 = " << num2 << endl;

    return 0;
}
